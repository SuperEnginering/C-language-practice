#include <stdio.h>
#include <string.h>

int main()
{ 
	char arr[10005];
	
    while(gets(arr)!=0)
    {
    	int i;
    	int len=strlen(arr);
    	for(i=0;i<len;i++)
    	{
    		if(arr[i]==' ')
    		{
    			printf("\n");
    			i++;
			}
			printf("%c",arr[i]);
			if(i==len-1) 	printf("\n");
		}
	printf("\n");
    }
    
    return 0;
}


