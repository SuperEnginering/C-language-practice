#include <stdio.h>
#include <string.h>

int main()
{ 
	int T;
	scanf("%d",&T);
	getchar();
	while(T--)
	{
		char str[21];
		gets(str);		
		int len=strlen(str);
//		char arr[len][10];
		int i;
		for(i=0;i<len;i++)
		{
			switch(str[i])
			{
				case '0':
					{
						printf("zero");
						break;	
					}
				case '1':
					{
						printf("one");
						break;	
					}
				case '2':
					{
						printf("two");
						break;	
					}
				case '3':
					{
						printf("three");
						break;	
					}
				case '4':
					{
						printf("four");
						break;	
					}
				case '5':
					{
						printf("five");
						break;	
					}
				case '6':
					{
						printf("six");
						break;	
					}
				case '7':
					{
						printf("seven");
						break;	
					}
				case '8':
					{
						printf("eight");
						break;	
					}
				case '9':
					{
						printf("nine");
						break;	
					}
													
			}			
			//if(i!=0||i!=len)
			 if(str[i]!=' ')
			 printf(" ");
		}
		printf("\n");
	}
    return 0;
}


