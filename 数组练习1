#include <string.h>
#include <stdio.h>

int main()
{ 
	int T;
	scanf("%d",&T);
	while(T--)
	{
		int n,k;
		scanf("%d%d",&n,&k);
		int arr[n];
		int i;
		for(i=1;i<=n;i++)
		{
			scanf("%d",&arr[i]);
		}		
//		int* p=&arr[0];
		int flag=0;
		for(i=1;i<=n;i++)
		{
			if(k==arr[i])
			{
				flag=i;
				break;
			}
		
		}
		printf("%d\n",flag);
	}
	
    return 0;
}


