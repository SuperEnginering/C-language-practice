#include <stdio.h>
int ack(int m,int n)
{
	//n+1 m==0
	if( m == 0 )
	{
		return n+1;
	}
	//ack(m-1,1)  n==0
	if( n==0 )
	{
		return ack(m-1,1);
	 } 
	//ack(m-1,ack(m,n-1))  m>0&&n>0
	if( m>0 && n>0 )
	{
		return ack(m-1,ack(m,n-1));	
	}	
}
int main(void) {
    int m, n;
    scanf("%d%d", &m, &n);
    printf("%d\n", ack(m, n)); 
    return 0;
}
